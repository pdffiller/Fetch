{
    "name": "pdffiller/fetch",
    "description": "A PHP IMAP Library",
    "keywords": [
        "email",
        "imap",
        "pop3"
    ],
    "homepage": "https://github.com/pdffiller/Fetch",
    "type": "library",
    "license": "MIT",

    "authors": [
        {
            "name": "Serghei Iakovlev",
            "email": "i.serghei@pdffiller.com",
            "role": "Developer"
        },
        {
            "name": "Robert Hafner",
            "email": "tedivm@tedivm.com"
        }
    ],

    "require": {
        "php": ">=7.0",
        "ext-imap": "*"
    },

    "require-dev": {
        "symfony/console": "3.4.*",
        "symfony/debug": "3.4.*",
        "symfony/finder": "3.4.*",
        "symfony/yaml": "3.4.*",
        "symfony/process": "3.4.*",
        "phpunit/phpunit": "6.5.*"
    },

    "autoload": {
        "psr-0": {
            "Fetch": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Fetch\\Tests\\": "tests/Fetch/"
        }
    }
}
